{
    "compilerOptions": {
      "target": "es2016",
      "lib": ["es6"],
      "module": "commonjs",
      "outDir": "build",
      "resolveJsonModule": true,
      "allowJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "skipLibCheck": true
    }
  }
  